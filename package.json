{
  "name": "discordinated-slacking",
  "version": "1.0.0",
  "description": "An integration between discord and slack using webhooks.",
  "main": "dist/index.js",
  "repository": "git@github.com:jimboslicethat/discordinated-slacking.git",
  "author": "James Walsh <jamesandersonwalsh@gmail.com>",
  "license": "LGPL-3.0-only",
  "scripts": {
    "build": "tsc",
    "start:dev": "TS_NODE_TRANSPILE_ONLY=true nodemon --inspect ./src/index.ts",
    "start": "node ./dist/index.js",
    "lint": "eslint . --ext .ts",
    "test": "echo 'No Tests yet :('"
  },
  "dependencies": {
    "axios": "0.19.2",
    "discord.js": "12.0.2",
    "dotenv": "8.2.0"
  },
  "devDependencies": {
    "@types/chai": "4.2.10",
    "@types/chai-as-promised": "7.1.2",
    "@types/mocha": "7.0.2",
    "@types/node": "13.9.1",
    "@types/sinon": "7.5.2",
    "@types/sinon-chai": "3.2.3",
    "@typescript-eslint/eslint-plugin": "2.23.0",
    "@typescript-eslint/parser": "2.23.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.20.1",
    "husky": "4.2.3",
    "mocha": "7.1.0",
    "nodemon": "2.0.2",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1",
    "sinon": "9.0.1",
    "sinon-chai": "3.5.0",
    "ts-node": "8.6.2",
    "typescript": "3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && pretty-quick --staged && npm run test && npm run build"
    }
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      ".git",
      "node_modules/**/node_modules"
    ],
    "verbose": true,
    "execMap": {
      "ts": "node --require ts-node/register"
    },
    "watch": [
      "src/"
    ],
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "json,ts"
  },
  "resolutions": {
    "minimist": "^1.2.2"
  }
}
